{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "types": ["@cloudflare/workers-types", "node"],
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "noEmit": true
  },
  "exclude": ["public"]
}
