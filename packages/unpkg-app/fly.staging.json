{
  "app": "unpkg-app-staging",
  "primary_region": "lax",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "NODE_ENV": "staging"
    }
  },
  "http_service": {
    "internal_port": 3000,
    "auto_stop_machines": "stop",
    "auto_start_machines": true,
    "min_machines_running": 0,
    "processes": ["app"]
  },
  "vm": [{ "size": "performance-1x" }]
}
