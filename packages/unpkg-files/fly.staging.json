{
  "app": "unpkg-staging",
  "primary_region": "lax",
  "build": {
    "dockerfile": "./Dockerfile",
    "args": {
      "MODE": "staging"
    }
  },
  "http_service": {
    "internal_port": 3000,
    "auto_start_machines": true,
    "min_machines_running": 1,
    "processes": ["app"],
    "checks": [
      {
        "grace_period": "10s",
        "interval": "1m0s",
        "method": "get",
        "path": "/_health",
        "timeout": "5s"
      }
    ]
  },
  "vm": [{ "size": "performance-1x" }]
}
